.table {
    border: 1px solid #e8e8e8;
    border-collapse: collapse;
    width: 100%;
    .tableRow {
    }
    .tableCell {
      color: #333;
      padding: 10px 15px;
      border: 1px solid #e8e8e8;
      font-weight: normal;
      vertical-align: middle;
      word-break: break-all;
      white-space: normal;
    }
    :global {
      tbody tr:nth-child(even) {
        td {
          background-color: #ebfbf2;
        }
      }
    }
    .picList{
      display: flex;
      justify-content: center;
      align-items: flex-start;
      text-align: center;
    }
    .picItem {
      width: 25%;
      :global {
        img {
          max-width: 100%;
          max-height: 100%;
        }
      }
    }
    .picBox{
      padding: 10px;
      text-align: center;
      height: 150px;
      display: flex;
      justify-content: center;
      align-items: center;
    }
  
  }
  