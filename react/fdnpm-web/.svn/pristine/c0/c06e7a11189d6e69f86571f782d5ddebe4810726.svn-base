.table {
  border: 1px solid #000;
  border-collapse: collapse;
  width: 100%;
  margin-left: 20px;
  margin-bottom: 20px;

  .tableRow {
  }

  .tableCell {
    color: #333;
    padding: 10px 15px;
    border: 1px solid #e8e8e8;
    font-weight: normal;
    vertical-align: middle;
    word-break: break-all;
    white-space: normal;
    width: 18%;
    text-align: center;
  }

  .tableCell1 {
    color: #333;
    padding: 10px 15px;
    border: 1px solid #e8e8e8;
    font-weight: normal;
    vertical-align: middle;
    word-break: break-all;
    white-space: normal;
    width: 18%;
    text-align: center;
  }

  .tableCellContent {
    color: #333;
    padding: 10px 15px;
    border: 1px solid #e8e8e8;
    font-weight: normal;
    vertical-align: middle;
    word-break: break-all;
    white-space: normal;
    width: 80%;
  }

  :global {
    tbody tr:nth-child(even) {
      td {
        background-color: #F7F9FB;
      }
    }
  }

  .picList {
    display: flex;
    justify-content: center;
    align-items: flex-start;
    text-align: center;
  }

  .picItem {
    width: 25%;

    :global {
      img {
        max-width: 100%;
        max-height: 100%;
      }
    }
  }

  .picBox {
    padding: 10px;
    text-align: center;
    height: 150px;
    display: flex;
    justify-content: center;
    align-items: center;
  }

}

.baseFormSpan {
  line-height: 40px;
  margin: 0 3px;
}

.baseFormSpanSegmentation {
  line-height: 40px;
  margin: 0 8px;
}